{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "hsse-admin-client",
      "image": "destinyforge/hsse-admin-client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "hsse-server",
      "image": "destinyforge/hsse-server",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "destinyforge/hsse-nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["hsse-admin-client", "hsse-server"],
      "memory": 128
    }
  ]
}
