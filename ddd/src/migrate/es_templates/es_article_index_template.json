{
  "settings": {
    "index": {
      "analysis": {
        "analyzer": {
          "nGram_analyzer": {
            "tokenizer": "whitespace",
            "filter": [
              "lowercase",
              "asciifolding",
              "nGram_filter"
            ],
            "type": "custom"
          },
          "whitespace_analyzer": {
            "tokenizer": "whitespace",
            "filter": [
              "lowercase",
              "asciifolding"
            ],
            "type": "custom"
          },
          "autocmp": {
            "tokenizer": "edgeNGram",
            "filter": [
              "lowercase"
            ],
            "type": "custom"
          },
          "splcharanalyzer": {
            "tokenizer": "keyword",
            "filter": [
              "splcharfilter",
              "lowercase"
            ],
            "type": "custom"
          }
        },
        "filter": {
          "nGram_filter": {
            "token_chars": [
              "letter",
              "digit",
              "punctuation",
              "symbol"
            ],
            "min_gram": 1,
            "max_gram": 20,
            "type": "nGram"
          },
          "splcharfilter": {
            "patterns": [
              "([?/])"
            ],
            "preserve_original": true,
            "type": "pattern_capture"
          }
        },
        "tokenizer": {
          "edgeNGram": {
            "min_gram": 1,
            "max_gram": 20,
            "type": "edgeNGram"
          }
        }
      }
    }
  },
